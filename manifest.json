{
  "manifest_version": 2,

  "name": "Whatsapp Media Hidden",
  "description": "This extension hides (blurred preview) WhatsAppâ„¢ media content and . (Images, GIF's and videos preview)",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
